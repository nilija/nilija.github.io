        + ' * @license       http://opensource.org/licenses/bsd-license.php The BSD License\n'
        + ' *\n'
        + ' * filename:      ' + fileName + '\n'
        + ' * generated:     ' + currentDateTime() + '\n'
        + ' */\n';
};
